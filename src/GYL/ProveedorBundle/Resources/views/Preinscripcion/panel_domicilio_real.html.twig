<div class="panel-heading bg-primary" role="button" data-toggle="collapse" href="#dom-real"
     aria-expanded="false" aria-controls="collapseExample"><h5>Domicilio Real</h5></div>
<div class="panel-body collapse" id="dom-real">
    <div class="col-md-12">
        {{ form_start(domicilioFormReal ,{'attr': {'id': 'domicilioFormReal', 'class': ' col-lg-11 col-lg-offset-1 datos-registro pad-top'}} ) }}
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label for="domicilio-provincia" class="control-label" style="float:left;"
                           id="label-provincia"><span class="text-danger">*</span>Provincia</label>
                    <div class="col-md-8 col-sm-12">
                    {% if estadoEvaluacionGral is null or (estadoEvaluacionGral == 4 and observacionEvaluacion is not null) or unlockForm  %}

                            {{ form_widget(domicilioFormReal.provincias_real,{ 'attr': {'class': 'form-control'} }) }}
                            {{ form_errors(domicilioFormReal.provincias_real) }}
                        </div>
                    {% else %}
                            {{ form_widget(domicilioFormReal.provincias_real,{ 'attr': {'disabled':'disabled'} }) }}
                            {{ form_errors(domicilioFormReal.provincias_real) }}
                        </div>
                    {% endif %}
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label for="domicilio-provincia" class="control-label ml-fl-domicilio"
                           id="label-localidad"><span class="text-danger">*</span>Localidad</label>
                    <div class="col-md-8 col-sm-12">
                    {% if estadoEvaluacionGral is null or (estadoEvaluacionGral == 4 and observacionEvaluacion is not null) or unlockForm  %}
                            <select required class="select-pronvicia form-control" name="state" id="selected-localidad-real">
                                <option value="">Elija una localidad</option>
                            </select>
                        </div>
                    {% else %}
                            <select required class="select-pronvicia form-control" name="state" id="selected-localidad-real" disabled="disabled">
                                <option value="">Elija una localidad</option>
                            </select>
                        </div>
                    {% endif %}
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label class="control-label" style="float:left;"
                           id="label-provincia"><span class="text-danger">*</span>Código Postal</label>
                     <div class="col-md-8 col-sm-12">
                    {% if estadoEvaluacionGral is null or (estadoEvaluacionGral == 4 and observacionEvaluacion is not null) or unlockForm  %}
                            {{ form_widget(domicilioFormReal.codigo_postal,{ 'attr': {'class': 'form-control'} }) }}
                            {{ form_errors(domicilioFormReal.codigo_postal) }}
                        </div>
                    {% else %}
                            {{ form_widget(domicilioFormReal.codigo_postal,{ 'attr': {'disabled':'disabled'} }) }}
                            {{ form_errors(domicilioFormReal.codigo_postal) }}
                        </div>
                    {% endif %}
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label class="control-label ml-fl-domicilio"
                           id="label-calle"><span class="text-danger">*</span>Calle/Nro de calle</label>
                    <div class="col-md-8 col-sm-12">
                    {% if estadoEvaluacionGral is null or (estadoEvaluacionGral == 4 and observacionEvaluacion is not null) or unlockForm  %}
                            {{ form_widget(domicilioFormReal.calle,{ 'attr': {'class': 'form-control'} }) }}
                            {{ form_errors(domicilioFormReal.calle) }}
                        </div>
                    {% else %}
                            {{ form_widget(domicilioFormReal.calle,{ 'attr': {'disabled':'disabled'} }) }}
                            {{ form_errors(domicilioFormReal.calle) }}
                        </div>
                    {% endif %}
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label class="control-label" style="float:left;"
                           id="label-piso">Piso</label>
                    <div class="col-md-8 col-sm-12">
                    {% if estadoEvaluacionGral is null or (estadoEvaluacionGral == 4 and observacionEvaluacion is not null) or unlockForm  %}
                            {{ form_widget(domicilioFormReal.piso,{ 'attr': {'class': 'form-control'} }) }}
                            {{ form_errors(domicilioFormReal.piso) }}
                        </div>
                    {% else %}
                            {{ form_widget(domicilioFormReal.piso,{ 'attr': {'disabled':'disabled'} }) }}
                            {{ form_errors(domicilioFormReal.piso) }}
                        </div>
                    {% endif %}
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label class="control-label ml-fl-domicilio" style="float:left;"
                           id="label-dpto">Dpto</label>
                    <div class="col-md-8 col-sm-12">             
                    {% if estadoEvaluacionGral is null or (estadoEvaluacionGral == 4 and observacionEvaluacion is not null) or unlockForm  %}
                            {{ form_widget(domicilioFormReal.departamento,{ 'attr': {'class': 'form-control'} }) }}
                            {{ form_errors(domicilioFormReal.departamento) }}
                        </div>
                    {% else %}
                            {{ form_widget(domicilioFormReal.departamento,{ 'attr': {'disabled':'disabled'} }) }}
                            {{ form_errors(domicilioFormReal.departamento) }}
                        </div>
                    {% endif %}
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label class="control-label" style="float:left;"
                           id="label-telefono"><span class="text-danger">*</span>Teléfono</label>
                     <div class="col-md-8 col-sm-12">
                    {% if estadoEvaluacionGral is null or (estadoEvaluacionGral == 4 and observacionEvaluacion is not null) or unlockForm  %}
                            {{ form_widget(domicilioFormReal.telefono,{ 'attr': {'class': 'form-control'} }) }}
                            {{ form_errors(domicilioFormReal.telefono) }}
                        </div>
                    {% else %}
                            {{ form_widget(domicilioFormReal.telefono,{ 'attr': {'disabled':'disabled'} }) }}
                            {{ form_errors(domicilioFormReal.telefono) }}
                        </div>
                    {% endif %}
                </div>
            </div>
        </div>
    </div>
        {{ form_end(domicilioFormReal) }}

    {% if observacionEvaluacion is not null %}
        {% include '@ProveedorBundle/Resources/views/Partials/observacion.html.twig' %}
    {%endif %}

    <div class="text-right">
        {% if estadoEvaluacionGral is null or (estadoEvaluacionGral == 4 and observacionEvaluacion is not null) or unlockForm  %}
            <button type="submit" class="btn btn-info  btn-borrador" id="submitDomicilioFormReal">
                <span> {% image '@saveldpi1' %}
                        <img class="image-responsive" src="{{ asset_url }}">
                    {% endimage %}</span></button>
        {% endif %}
    </div>


</div>
